@using RealEstates.Models
@model RealEstateProperty
<h1 class="text-center">You succssesfully created Property.</h1>
<table class="table table-striped table-hover table-sm">
    <tr>
        <th>District</th>
        <th>Buildingtype</th>
        <th>PropertyType</th>
        <th>Price</th>
        <th>Year</th>
        <th>Size</th>
        <th>Floor</th>
    <tr />
    <tr>
        <td> @Model.District.Name</td>
        <td> @Model.BuildingType.Name</td>
        <td> @Model.PropertyType.Name</td>
        <td> @Model.Price.ToString("0.00")€</td>
        <td> @Model.Year</td>
        <td> @Model.Size</td>
        <td> @Model.Floor.ToString()/@Model.TotalFloors.ToString()</td>
    </tr>

